#! /usr/local/bin/vvp
:ivl_version "0.10.0 (devel)" "(s20111127)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x9825670 .scope module, "hello_world" "hello_world" 2 8;
 .timescale 0 0;
    .scope S_0x9825670;
T_0 ;
    %vpi_call 2 11 "$display", "Hello World by Deepak";
    %delay 10, 0;
    %vpi_call 2 12 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello_world.v";
