#! /bin/sh

cmd="cat iperf-tcp-cpu.log | sed -n "
i=1
while [ $i -lt 301]
do
	t="-e "
	t=$t$i
	t=$t"p "
	cmd=$cmd$t
	i=$(expr $i + 12)
done
cmd=$cmd" | cut -f 4 -d ' '"

sh $cmd

cat iperf-tcp-cpu.log | sed -n -e 1p -e 7p


